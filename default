server {
    listen 80;
    listen [::]:80;

    server_name cashflow.hanasand.com;
    return 301 https://pengeflyt.com$request_uri;
}

server {
    listen 80;
    listen [::]:80;

    server_name api.cashflow.hanasand.com;
    return 301 https://api.pengeflyt.com$request_uri;
}

server {
    listen 80;
    listen [::]:80;

    server_name *.hanasand.com;
    return 301 https://$host$request_uri;
}

server {
    listen 443 ssl;
    listen [::]:443 ssl;
    ssl_certificate /etc/letsencrypt/live/hanasand.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

    server_name cashflow.hanasand.com;
    return 301 https://pengeflyt.com$request_uri;
}

