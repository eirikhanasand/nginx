server {
    if ($host = hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = www.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = name-pending-server.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = name-pending.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = idk-api.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = idk.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = lsm-api.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = lsm.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = gitlab.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = speedrun-api.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = speedrun.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = eirik.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = api.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = docs.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = cdn.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    server_name *.hanasand.com;

    return 301 https://$host$request_uri;

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/hanasand.com-0001/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com-0001/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}

server {
    listen 443 ssl;
    server_name hanasand.com www.hanasand.com;
    ssl_certificate /etc/letsencrypt/live/hanasand.com-0002/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com-0002/privkey.pem; # managed by Certbot

    location / {
        proxy_pass http://localhost:3000;
        include snippets/proxy-headers.conf;
    }


}

server {
    listen 443 ssl;
    server_name docs.hanasand.com;
    ssl_certificate /etc/letsencrypt/live/hanasand.com-0002/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com-0002/privkey.pem; # managed by Certbot

    location / {
        proxy_pass http://localhost:3002;
        include snippets/proxy-headers.conf;
    }

}

server {
    listen 443 ssl;
    server_name api.hanasand.com;
    ssl_certificate /etc/letsencrypt/live/hanasand.com-0002/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com-0002/privkey.pem; # managed by Certbot

    location / {
        proxy_pass http://localhost:8080;
        include snippets/proxy-headers.conf;
    }

}

