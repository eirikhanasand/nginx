    }
}

server {
    listen 443 ssl;
    server_name cashflow.hanasand.com;

    ssl_certificate /etc/letsencrypt/live/hanasand.com/fullchain.pem;
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com/privkey.pem;

    location / {
        proxy_pass http://localhost:10001;
        include snippets/proxy-headers.conf;
    }
}

stream {
    map $ssl_preread_server_name $backend {
        pengeflyt.com     127.0.0.1:10001;
        www.pengeflyt.com 127.0.0.1:10001;
        api.pengeflyt.com 127.0.0.1:10001;
        default           127.0.0.1:443;
    }

    server {
        listen 443;
        proxy_pass $backend;

        ssl_preread on;
    }
}
