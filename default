server {
    listen 443 ssl;
    server_name name-pending-server.hanasand.com;
    ssl_certificate /etc/letsencrypt/live/hanasand.com-0002/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com-0002/privkey.pem; # managed by Certbot

    location / {
        proxy_pass http://localhost:8100;
        include snippets/proxy-headers.conf;
    }

}

server {
    listen 443 ssl;
    server_name name-pending.hanasand.com;
    ssl_certificate /etc/letsencrypt/live/hanasand.com-0002/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com-0002/privkey.pem; # managed by Certbot

    location / {
        if ($http_user_agent !~* "java") {
            return 401;
        }

        proxy_pass http://localhost:6969;
        include snippets/proxy-headers.conf;
    }

}

server {
    listen 443 ssl;
    server_name cdn.hanasand.com;

    ssl_certificate /etc/letsencrypt/live/hanasand.com/fullchain.pem;
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com/privkey.pem;

    location / {
        proxy_pass http://localhost:8501;
        include snippets/proxy-headers.conf;
    }
}

server {
    listen 443 ssl;
    server_name pengeflyt.com;
    proxy_ssl_verify off;

    location / {
        proxy_pass http://localhost:10001;
        include snippets/proxy-headers.conf;
    }
}

