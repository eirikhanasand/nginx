server {
    if ($host = name-pending-server.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = name-pending.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = idk-api.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = idk.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = lsm-api.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = lsm.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = gitlab.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = speedrun-api.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = speedrun.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = eirik.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = api.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = docs.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = cdn.hanasand.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    listen 80;
    server_name *.hanasand.com;
    return 404; # managed by Certbot
}
