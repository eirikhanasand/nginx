server {
    listen 443 ssl;
    server_name name-pending.hanasand.com;
    ssl_certificate /etc/letsencrypt/live/hanasand.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/hanasand.com/privkey.pem; # managed by Certbot

    location / {
        if ($http_user_agent !~* "java") {
            return 401;
        }

        proxy_pass http://localhost:6969;
        include snippets/proxy-headers.conf;
    }

}

